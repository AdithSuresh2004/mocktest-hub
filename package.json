{
  "name": "mocktest-hub",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "generate": "node scripts/generate-manifest.js && node scripts/validate-exams.js",
    "prebuild": "npm run generate",
    "lint": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css}\" \"scripts/**/*.js\" \"*.{js,ts,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css}\" \"scripts/**/*.js\" \"*.{js,ts,json,css}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "analyze": "vite build --mode analyze",
    "analyze:open": "npm run analyze && open dist/stats.html",
    "size": "npm run build && du -sh dist && du -sh dist/assets/*.js | sort -h"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "idb": "^8.0.3",
    "immer": "^10.2.0",
    "katex": "^0.16.25",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-katex": "^3.1.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^2.13.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.1.0"
  },
  "homepage": "https://mocktest-hub.vercel.app/"
}
